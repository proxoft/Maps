<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <RootNamespace>Proxoft.Maps.OpenStreetMap.Maps</RootNamespace>
        <AssemblyName>Proxoft.Maps.OpenStreetMap.Maps</AssemblyName>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Proxoft</Authors>
        <Company>Proxoft s.r.o.</Company>
        <Product>Maps</Product>
        <PackageId>Proxoft.Maps.OpenStreetMap.Maps</PackageId>
        <Version>0.0.2</Version>
        <PackageProjectUrl>https://github.com/proxoft/Maps</PackageProjectUrl>
        <PackageIcon>Map.png</PackageIcon>
    </PropertyGroup>

    <ItemGroup>
        <SupportedPlatform Include="browser" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\Maps.Core\Maps.Core.csproj" />
    </ItemGroup>

    <ItemGroup>
      <None Include="..\..\Map.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

</Project>
