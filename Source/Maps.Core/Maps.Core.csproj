        <RootNamespace>Proxoft.Maps.Core</RootNamespace>
        <AssemblyName>Proxoft.Maps.Core</AssemblyName>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Proxoft</Authors>
        <Company>Proxoft s.r.o.</Company>
        <Product>Maps</Product>
        <PackageIcon>Map.png</PackageIcon>
        <Version>1.0.0</Version>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\Map.png">
