      <PackageReference Include="Microsoft.JSInterop" Version="5.0.0" />
      <PackageReference Include="Proxoft.Extensions.Options" Version="1.2.0" />
      <PackageReference Include="Proxoft.Extensions.ValueObjects" Version="1.0.2" />
      <PackageReference Include="System.Reactive" Version="5.0.0" />
